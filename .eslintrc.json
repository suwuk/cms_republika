{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "linebreak-style": 0,
        "camelcase": ["error", { "allow": ["user_id","id_parent","role_id","menu_id","category_id"] }]
    }
}
